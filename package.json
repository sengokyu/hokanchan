    "axios-observable": "^1.1.2",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.4",
    "@angular/cli": "~8.3.4",
    "@angular/compiler-cli": "~9.0.0-next.6",
    "@angular/language-service": "~9.0.0-next.6",
    "@types/aws-lambda": "^8.10.31",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/xml2js": "^0.4.4",
    "aws-sam-webpack-plugin": "^0.3.1",
    "codelyzer": "^5.0.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-typescript": "^4.1.1",
    "protractor": "~5.4.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "~5.15.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "scripts": {
    "ng": "ng",
    "ng:start": "ng serve --proxy-config proxy.conf.json",
    "ng:build": "ng build --prod",
    "ng:test": "ng test",
    "ng:lint": "ng lint",
    "ng:e2e": "ng e2e",
    "s3:sync": "aws s3 sync dist/frontend-hokanchan  s3://hokanchan.uart.dev",
    "s3:delete": "aws s3 rm s3://hokanchan.uart.dev --recursive",
    "sam:build": "webpack-cli",
    "sam:watch": "webpack-cli -w",
    "sam:start": "sam local start-api",
    "sam:test": "karma start projects/backend-google-suggest-proxy/karma.conf.js",
    "sam:validate": "sam validate --template template.yaml",
    "sam:package": "npm run sam:build;sam package --template-file .aws-sam/build/template.yaml --s3-bucket lambdastash --output-template-file /tmp/packaged.yaml",
    "sam:deploy": "npm run sam:package;sam deploy --region us-west-1 --template-file /tmp/packaged.yaml --stack-name hokanchan --capabilities CAPABILITY_IAM",
    "sam:describe-stack-events": "aws --region us-west-1 cloudformation describe-stack-events --stack-name hokanchan",
    "sam:describe-failed-events": "aws --region us-west-1 cloudformation describe-stack-events --stack-name hokanchan --query 'StackEvents[?ResourceStatus==`CREATE_FAILED`]'",
    "sam:apiurl": "aws --region us-west-1 cloudformation describe-stacks --stack-name hokanchan --query 'Stacks[].Outputs[?OutputKey==`GoogleSuggestProxyApi`]' --output table",
    "sam:delete-stack": "aws --region us-west-1 cloudformation delete-stack --stack-name hokanchan",
    "sam:wait-delete-stack": "aws --region us-west-1 cloudformation wait stack-delete-complete --stack-name hokanchan",
    "sam:undeploy": "npm run sam:delete-stack;npm run sam:wait-delete-stack"
  },
  "author": "",
  "license": "ISC"
}
